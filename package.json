{
  "name": "@sighmir/altvrp",
  "version": "1.0.0",
  "description": "alt:V RP framework",
  "scripts": {
    "start": "cd ../.. && altv-server.exe",
    "start:dev": "yarn build && yarn start",
    "build": "npx del -f dist && npx babel src --out-dir dist --extensions .ts --copy-files && cd view && yarn build && cd ..",
    "watch": "npx del -f dist && npx babel --watch src --out-dir dist --extensions .ts --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sighmir/altv-proxy.git"
  },
  "keywords": [
    "sighmir",
    "vrp",
    "altmp",
    "altv",
    "node",
    "node-module",
    "js-module",
    "gta",
    "gta5",
    "gtav"
  ],
  "author": "Sighmir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sighmir/altv-proxy/issues"
  },
  "homepage": "https://github.com/Sighmir/altv-proxy#readme",
  "type": "module",
  "devDependencies": {
    "@altv/types-client": "^1.7.7",
    "@altv/types-natives": "^1.4.2",
    "@altv/types-server": "^1.9.8",
    "@altv/types-shared": "^1.0.1",
    "@altv/types-webview": "^1.0.2",
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.5",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "copyfiles": "^2.4.1",
    "del-cli": "^4.0.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1"
  }
}
